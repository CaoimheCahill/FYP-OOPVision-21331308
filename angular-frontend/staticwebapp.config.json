{
  "routes": [
    {
      "route": "/*",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "responseOverrides": {
    "/index.html": {
      "replace": {
        "%apiBaseUrl%": "apiBaseUrl"
      }
    }
  }
}
