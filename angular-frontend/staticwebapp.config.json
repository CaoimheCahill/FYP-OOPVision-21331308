{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "responseOverrides": {
    "/assets/config.json": {
      "replace": {
        "%API_BASE_URL%": "apiBaseUrl"
      }
    }
  }
}
