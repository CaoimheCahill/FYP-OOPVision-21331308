{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/staticwebapp.config.json",
      "/*.js",
      "/*.css",
      "/*.ico",
      "/*.png",
      "/*.woff",
      "/*.woff2"
    ]
  }
}
